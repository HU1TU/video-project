<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.tt.dao.CourseDao">
    <select id="selectCourseById" resultType="com.tt.pojo.Course">
        select * from video.course where id = #{id}
    </select>

    <select id="selectAll" resultType="com.tt.pojo.Course">
        select * from video.course
    </select>

</mapper>