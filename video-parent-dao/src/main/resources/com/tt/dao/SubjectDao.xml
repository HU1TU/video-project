<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.tt.dao.SubjectDao">
    <select id="selectAll" resultType="com.tt.pojo.Subject">
        select * from video.subject
    </select>

    <select id="selectSubjectById" resultType="com.tt.pojo.Subject">
        select * from video.subject where id = #{id}
    </select>

</mapper>